{
  "name": "tsapi",
  "version": "2.0.0",
  "description": "API TypeScript para gestÃ£o de vendas e estoque - Sistema interno",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "node ./node_modules/typescript/bin/tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "test:config-inatividade": "ts-node scripts/testConfiguracaoInatividade.ts",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "heroku-postbuild": "npm install --include=dev && npm run build",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "typescript",
    "express",
    "prisma",
    "mysql",
    "api",
    "rest"
  ],
  "author": "Desenvolvimento Interno",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@prisma/client": "^5.15.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-layers": "^4.22.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/graphql": "^14.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.25",
    "prisma": "^5.15.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
