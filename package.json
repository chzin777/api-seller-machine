{
  "name": "tsapi",
  "version": "2.0.0",
  "description": "API TypeScript para gest√£o de vendas e estoque - Sistema interno",
  "main": "dist/index.js",
  "scripts": {
  "start": "node dist/index.js",
  "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
  "build": "prisma generate && npx tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "heroku-postbuild": "npm install --include=dev && npm run build"
  },
  "keywords": [
    "typescript",
    "express",
    "prisma",
    "mysql",
    "api",
    "rest"
  ],
  "author": "Desenvolvimento Interno",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.25",
    "prisma": "^5.10.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
